[Unit]
Description=RPi DDF Engine
Requires=rpi-ddf-engine.socket
After=rpi-ddf-engine.socket


[Service]
Type=simple
User=root
Group=root
ExecStart=/usr/bin/stdbuf -o0 /usr/local/bin/RPiDDFengine
StandardOutput=socket
WorkingDirectory=/usr/local/bin
#CPUAffinity=2
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target

